<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="800" height="500" viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
  <rect width="100%" height="100%" fill="#e9ecef"/>
  <rect x="50" y="50" width="700" height="400" rx="20" ry="20" fill="white" stroke="#dee2e6" stroke-width="2"/>
  
  <!-- Header -->
  <text x="400" y="90" font-family="Arial, sans-serif" font-size="24" text-anchor="middle" fill="#212529">Phonetic Keyboard Explorer</text>
  
  <!-- Image container -->
  <rect x="250" y="120" width="300" height="200" rx="10" ry="10" fill="#4dabf7" stroke="#dee2e6" stroke-width="2"/>
  <text x="400" y="220" font-family="Arial, sans-serif" font-size="120" font-weight="bold" text-anchor="middle" fill="white">A</text>
  <text x="400" y="270" font-family="Arial, sans-serif" font-size="24" text-anchor="middle" fill="white">Alligator</text>
  
  <!-- Letter display -->
  <circle cx="400" cy="350" r="50" fill="#f8f9fa" stroke="#dee2e6" stroke-width="2"/>
  <text x="400" y="370" font-family="Arial, sans-serif" font-size="60" font-weight="bold" text-anchor="middle" fill="#4dabf7">A</text>
  
  <!-- Instruction -->
  <text x="400" y="420" font-family="Arial, sans-serif" font-size="18" text-anchor="middle" fill="#6c757d">Press the letter A on your keyboard!</text>
  
  <!-- Progress bar -->
  <rect x="200" y="440" width="400" height="15" rx="7.5" ry="7.5" fill="#e9ecef"/>
  <rect x="200" y="440" width="15" height="15" rx="7.5" ry="7.5" fill="#4dabf7"/>
</svg>